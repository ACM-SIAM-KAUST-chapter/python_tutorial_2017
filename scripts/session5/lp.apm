Model

  Variables
    x[1] = 4, >=4
    x[2] = -80, >=-80, <=-8
    x[3] = 0  >=-1e10, <=1e10
  End Variables

  Equations
    x[1] + 2*x[2] + 3*x[3] <= 15
    8*x[1] + 15*x[2] + 80*x[3] <= 80
    8*x[1] + 80*x[2] + 15*x[3] <= 150
   -100*x[1] - 10*x[2] - x[3] <= -800
    80*x[1] + 8*x[2] + 15*x[3] = 750
    x[1] + 10*x[2] + 100*x[3] = 80 

    minimize  15*x[1] + 8*x[2] + 80*x[3]
  End Equations

End Model
